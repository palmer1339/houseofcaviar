
/* fix yui base problems */
#filemanager ul { margin: 0; }
#filemanager li { list-style: none; }
/* end yui base fix */

#tabview { visibility: hidden; }
#tabview .yui-nav { margin: 0; }

#tabview.yui-navset .yui-nav li.disabled a {
	color: #ccc;
}
#tabview.yui-navset .yui-nav li.disabled a:hover {
	cursor: default;
	background-position: 0 -200px !important;
}

#file_top {
	position: relative;
}
#file_top #act_throbber {
	position: absolute;
	top: 1px;
	right: 1px;
}
#file_top #act_throbber img {
	width: 28px;
	height: 28px;
}
#fldr_tlbr, #file_tlbr {
	height: 23px;
	padding: 3px 3px 4px;
	background: #fff;
	outline: 0;
}
#fldr_ftp,
#file_srch {
	height: 32px;
	padding-top: 1px;
	background: #d8d8d8;
	overflow: hidden;
}
#fldr_tlbr.yui-toolbar-container .yui-push-button .yui-toolbar-icon,
#file_tlbr.yui-toolbar-container .yui-push-button .yui-toolbar-icon,
