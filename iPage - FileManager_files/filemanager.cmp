#file_srch label.srch {
	padding: 3px 0 1px 3px;
	background: #fff;
	border: 1px solid #808080;
	margin: 1px 0 3px 0.5em;
}
#file_srch label.srch, #file_srch #file_srch_fltr2, #file_srch #file_srch_sys2 { float: left; }
#file_srch span.clear {
	display: inline-block;
	background: url(/generalAppC/javascripts/yui/assets/skins/sam/sprite.png) no-repeat scroll -6px -303px;
	width: 13px;
	height: 10px;
	cursor: pointer;
	margin: 0;
}
#file_srch label.srch input {
	border: 0;
	background: transparent;
	color: #333;
	font-size: 12px;
	height: auto;
	margin: 0;
	padding: 0;
}
#file_srch_sys2.yui-checkbox-button-checked {
	background-position: 0 -100px;
