#tpl_top_tlbr.yui-toolbar-container .yui-push-button .yui-toolbar-icon {
	background: url(/controlpanel/FileManager/styles/sprite.png) no-repeat scroll 0 0;
	margin-left: 5px
}
#fldr_tlbr.yui-toolbar-container .yui-toolbar-new .yui-toolbar-icon { background-position: -3px -1px; }
#fldr_tlbr.yui-toolbar-container .yui-toolbar-delete .yui-toolbar-icon { background-position: -24px -1px; }
